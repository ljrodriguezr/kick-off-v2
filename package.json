{
  "name": "kick-off-v2",
  "version": "1.0.0",
  "description": "Kick Off V2 Application",
  "main": "index.js",
  "repository": "https://github.com/wpleonesz/kick-off-v2.git",
  "author": "Kick Off V2 Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:pm2": "pm2 start ecosystem.config.js --only SGU_DEV",
    "dev:pm2:stop": "pm2 stop SGU_DEV",
    "dev:pm2:restart": "pm2 restart SGU_DEV",
    "dev:pm2:logs": "pm2 logs SGU_DEV",
    "dev:pm2:delete": "pm2 delete SGU_DEV",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev --skip-seed",
    "migrate-deploy": "prisma migrate deploy --skip-seed",
    "seed": "prisma db seed",
    "seed-dev": "node prisma/seed/dev.js",
    "format:check": "prettier --check components database pages prisma ui lib helper middleware module services validations redux hooks *.js *.json",
    "format:write": "prettier --write components database pages prisma ui lib helper middleware module services validations redux hooks *.js *.json",
    "lint": "eslint components database helper lib middleware module pages prisma redux services ui validations",
    "build:prod": "next build",
    "prod:start": "pm2 start ecosystem.config.js --only SGU_PROD --env production",
    "prod:stop": "pm2 stop SGU_PROD",
    "prod:restart": "pm2 restart SGU_PROD",
    "prod:logs": "pm2 logs SGU_PROD",
    "prod:delete": "pm2 delete SGU_PROD",
    "deploy:prod": "yarn build:prod && pm2 pull && yarn prod:start"
  },
  "prisma": {
    "seed": "node prisma/seed/configure.js",
    "seed-cardsavailable": "node prisma/seed/cardsavailable.js",
    "seed-terms": "node prisma/seed/terms.js",
    "seed-faq": "node prisma/seed/faq.js"
  },
  "private": true,
